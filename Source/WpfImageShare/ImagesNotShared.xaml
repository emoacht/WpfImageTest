<UserControl x:Class="WpfImageShare.ImagesNotShared"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:WpfImageShare"
			 mc:Ignorable="d"
			 d:DesignWidth="1200" d:DesignHeight="400">
	<UserControl.Resources>
		<Image x:Key="ImageKey" Source="pack://application:,,,/Resources/test.jpg"
			   x:Shared="False"/>
	</UserControl.Resources>
	<StackPanel Orientation="Horizontal">
		<Button>
			<StaticResource ResourceKey="ImageKey"/>
		</Button>
		<Button>
			<StaticResource ResourceKey="ImageKey"/>
		</Button>
	</StackPanel>
</UserControl>